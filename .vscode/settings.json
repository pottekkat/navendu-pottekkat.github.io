{
  "cSpell.words": ["influencers", "Organising"],
  "frontMatter.git.enabled": true,
  "[markdown]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.fontSize": 16,
    "editor.lineHeight": 26,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "editor.lineNumbers": "off",
    "editor.quickSuggestions": false,
    "editor.minimap.enabled": false,
    "editor.fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif"

  },
  "workbench.colorCustomizations": {
    "titleBar.activeBackground": "#111111",
    "titleBar.inactiveBackground": "#212121",
    "titleBar.activeForeground": "#f4f4f4",
    "titleBar.inactiveForeground": "#b4b4b4"
  }
}
